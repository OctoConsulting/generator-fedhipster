                        <!--<a href="#">Navigation link</a>-->
                    <!--</li>-->
                    <!--<li class="usa-nav__submenu-item">-->
                        <!--<a href="#">Navigation link</a>-->
                    <!--</li>-->
                <!--</ul>-->
            <!--</li>-->
        </ul>
        <div class="usa-nav__secondary">
            <form class="usa-search usa-search--small" style="display: flex">
                <a *ngIf="!isAuthenticated()" href="javascript:void(0)" (click)="login()" style="color: white; width: 4em; text-decoration: none">Sign in</a>
                <a *ngIf="isAuthenticated()" href="javascript:void(0)" style="color: white; width: 14em; text-decoration: none">Welcome, {{this.userName}}</a>
                <div role="search">
                    <label class="usa-sr-only" for="extended-search-field-small">Search small</label>
                    <input class="usa-input" id="extended-search-field-small" type="search" name="search" placeholder="Search our site">
